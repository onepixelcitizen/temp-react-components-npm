    "@types/styled-components": "4.1.8",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "install-peers-cli": "^2.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-typescript2": "^0.25.2",
    "styled-components": "^4.4.1",
